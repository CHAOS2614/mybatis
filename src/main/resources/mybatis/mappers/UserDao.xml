<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.bjfu.dao.UserDao">
    <select id="getUserById" resultType="cn.edu.bjfu.domain">
        select *
        from user
        where id = #{id}
    </select>
</mapper>